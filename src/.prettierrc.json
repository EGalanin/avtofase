{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ["*.ts", "*.tsx"],
            "options": {
                "singleQuote": false,
                "jsxSingleQuote": false
            }
        }
    ],
    "plugins": ["prettier-plugin-tailwindcss"]
}
